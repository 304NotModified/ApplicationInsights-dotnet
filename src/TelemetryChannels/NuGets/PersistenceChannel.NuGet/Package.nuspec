<?xml version="1.0"?>
<package >
  <metadata>
    <id>Microsoft.ApplicationInsights.PersistenceChannel</id>
    <version>$version$</version>
    <title>Application Insights Persisted HTTP channel</title>
    <authors>$author$</authors>
    <owners>$author$</owners>
    <licenseUrl>http://go.microsoft.com/fwlink/?LinkID=391182</licenseUrl>
    <projectUrl>http://go.microsoft.com/fwlink/?LinkId=392727</projectUrl>
    <iconUrl>$image$</iconUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>This channel provides a peristant channel to Application Insights that will preserve telemetry in offline scenarios. This is a dependent package, for a best experience please install the platform specific package.</description>
    <copyright>$copyright$</copyright>
    <releaseNotes>For the release notes please follow http://go.microsoft.com/fwlink/?LinkId=535037</releaseNotes>
    <tags>Analytics ApplicationInsights Telemetry</tags>
    <dependencies>
      <group>
        <dependency id="Microsoft.ApplicationInsights" version="$version$" />
      </group>
    </dependencies>
  </metadata> 
  <files>
    <!-- Binaries for .NET 4.0 projects -->
    <file src="$configuration$\TelemetryChannels\PersistenceChannel\Net40\Microsoft.ApplicationInsights.PersistenceChannel.dll" target="lib\net40" />
    <file src="$configuration$\TelemetryChannels\PersistenceChannel\Net40\Microsoft.ApplicationInsights.PersistenceChannel.pdb" target="lib\net40" />
    <file src="$configuration$\TelemetryChannels\PersistenceChannel\Net40\Microsoft.ApplicationInsights.PersistenceChannel.xml" target="lib\net40" />

    <!-- wp8: Binaries for Windows Phone 8.0 and 8.1 (Silverlight Runtime) projects -->
    <file src="$configuration$\TelemetryChannels\PersistenceChannel\Wp80\Microsoft.ApplicationInsights.PersistenceChannel.dll" target="lib\wp8" />
    <file src="$configuration$\TelemetryChannels\PersistenceChannel\Wp80\Microsoft.ApplicationInsights.PersistenceChannel.pdb" target="lib\wp8" />
    <file src="$configuration$\TelemetryChannels\PersistenceChannel\Wp80\Microsoft.ApplicationInsights.PersistenceChannel.xml" target="lib\wp8" />

    <!-- portable-win81+wpa81: Windows Runtime projects (Phone & Store, Managed)-->
    <file src="$configuration$\TelemetryChannels\PersistenceChannel\Wrt81\Microsoft.ApplicationInsights.PersistenceChannel.dll" target="lib\portable-win81+wpa81" />
    <file src="$configuration$\TelemetryChannels\PersistenceChannel\Wrt81\Microsoft.ApplicationInsights.PersistenceChannel.pdb" target="lib\portable-win81+wpa81" />
    <file src="$configuration$\TelemetryChannels\PersistenceChannel\Wrt81\Microsoft.ApplicationInsights.PersistenceChannel.xml" target="lib\portable-win8+wpa81" />
  </files>
</package>